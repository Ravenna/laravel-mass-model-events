{
  "name": "ravenna/mass-model-events-for-laravel",
  "description": "Model events for creating, updating, or deleting multiple models in Laravel",
  "authors": [
    {
      "name": "Dominic Sears",
      "email": "dominic@ravennainteractive.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/database": "^11.0|^12.0"
  },
  "require-dev": {
    "laravel/framework": "^11.0|^12.0",
    "orchestra/testbench-core": "^9.0|^10.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Ravenna\\MassModelEvents\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ravenna\\MassModelEvents\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
